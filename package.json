    "@types/papaparse": "^5.3.15",
    "@types/ramda": "^0.30.2",
    "@vitest/coverage-v8": "3.1.4",
    "@vitest/eslint-plugin": "^1.2.1",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^9.2.0",
