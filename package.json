    "papaparse": "5.3.2",
    "radix-vue": "^1.9.17",
    "ramda": "^0.30.1",
    "uuid": "^11.1.0",
    "vue": "^3.5.14",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
