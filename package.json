    "@tanstack/vue-table": "^8.21.3",
    "@vueuse/core": "^13.2.0",
    "color": "^5.0.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "electron-squirrel-startup": "^1.0.1",
