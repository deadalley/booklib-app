{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "main": "dist-electron/main.js",
  "version": "0.1.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --open --dotenv .env.local",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint:js": "eslint .",
    "lint:ts": "nuxi typecheck",
    "lint:prettier": "prettier . --check",
    "lint": "yarn lint:js && yarn lint:ts && yarn lint:prettier",
    "lint:fix": "prettier . --write --list-different && yarn lint:js --fix",
    "test": "vitest",
    "test:start-app": "nuxt preview --port 3001 --dotenv .env.test",
    "test:e2e": "yarn playwright test --project=chromium",
    "test:e2e:ui": "yarn playwright test --ui",
    "test:e2e:codegen": "concurrently 'yarn test:start-app' 'playwright codegen'",
    "test:e2e:report": "playwright show-report",
    "coverage": "vitest run --coverage",
    "story:dev": "histoire dev",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "dependencies": {
    "@formkit/nuxt": "^1.6.9",
    "@nuxt/eslint": "^1.4.1",
